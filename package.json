{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "start:server": "webpack --mode=development --config webpack/webpack.config.server.cjs",
    "start:client": "webpack --mode=development --config webpack/webpack.config.client.cjs",
    "start": "rm -rf dist/ && npm run start:client && npm run start:server && node dist/server.js",
    "startWatch": "npm-run-all --parallel startWatch:*",
    "startWatch:server": "nodemon --watch dist --exec node dist/server.js",
    "startWatch:build-server": "webpack --config webpack/webpack.config.server.cjs --watch",
    "startWatch:build-client": "webpack --config  webpack/webpack.config.client.cjs --watch --no-cache",
    "build:prod": "rm -rf ./dist && webpack --config webpack/webpack.config.server.cjs --config webpack/webpack.config.client.cjs",
    "build:dev": "rm -rf ./dist && webpack --config webpack/webpack.config.server.cjs --config webpack/webpack.config.client.cjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@loadable/babel-plugin": "^5.13.2",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
